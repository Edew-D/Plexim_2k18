          CodeFcn       "local Require = {}\nlocal InitCode = {}\nlocal Output"
"Code = {}\n\nprint(Block.Mask.out[1][1])\nprint(Block.Mask.invert[1][1])\n-- "
"Error Handling:\nif Block.Mask.out[1] == Block.Mask.invert[1] or Block.Mask.o"
"ut2[1] == Block.Mask.invert[1] then \nreturn \"Error: Output Pin and Inverted"
" Signal Pin cannot be the same\" \nend\n\nlocal function pin_set(pin, prescal"
"e)\n\ttable.insert(Require, string.format(\"PWM_Pin:%i\", pin))\n\tlocal inst"
"ance = Target.Coder.RegisterPwmOutBlock(pin)\n\tif instance == \"Error\" then"
" return \"Error: PWM Pin Must be 3, 5, 6, or 11\" end\n  \ttable.insert(Requi"
"re, string.format(\"GPIO:%i\", pin))\n  \ttable.insert(InitCode, string.forma"
"t(\"configDout(%i, %i);\", instance, pin))\n  \ttable.insert(InitCode, string"
".format(\"analog_init(%i, %i, %i);\", instance, prescale, inv))\n  \ttable.in"
"sert(OutputCode, string.format(\"set_analog(%i, %s);\", instance, Block.Input"
"Signal[1][1]))\nend\n\n\nlocal function pin_init(timer, pin0, pin2, prescale0"
", prescale2)\n\tlocal table0 = {5, 6}\n\tlocal table2 = {3, 5}\n\tlocal inv ="
" false\n\t\n\tif timer == 1 then\n\t\tlocal pin = pin0\n\t\tif pin ~= 3 then"
"\n\t\t\tpin_set(table0[pin], prescale0, inv)\n\t\telse\n\t\t\tpin_set(5, pres"
"cale0, inv)\n\t\t\tpin_set(6, prescale0, inv)\n\t\tend\n\t\t\n\telse\n\t\tloc"
"al pin = pin2\n\t\tif pin ~= 3 then\n\t\t\tpin_set(table2[pin], prescale2, in"
"v)\n\t\telse\n\t\t\tpin_set(3, prescale2, inv)\n\t\t\tpin_set(11, prescale2, "
"inv)\n\t\tend\n\tend\nend\n\n\nlocal function invert_init(timer, out0, out2, "
"invert_pin, prescale0, prescale2)\n\tlocal table0 = {5, 6}\n\tlocal table2 = "
"{3, 11}\n\tlocal table_invert = {3, 5, 6, 11}\n\tlocal inv = true\n\t\n\tif t"
"imer == 1 then\n\t\tlocal pin = out0\n\t\tif pin ~= 3 then\n\t\t\tpin_set(tab"
"le0[pin], prescale0, inv)\n\t\t\t--table.insert(InitCode, string.format(\"inv"
"ert_signal(%i, %i);\", instance, invert))\n\t\telse\n\t\t\tpin_set(5, prescal"
"e0, inv)\n\t\t\tpin_set(6, prescale0, inv)\n\t\tend\n\t\t\n\telse\n\t\tlocal "
"pin = out2\n\t\tif pin ~= 3 then\n\t\t\tpin_set(table2[pin], prescale2, inv)"
"\n\t\telse\n\t\t\tpin_set(3, prescale2, inv)\n\t\t\tpin_set(11, prescale2, in"
"v)\n\t\tend\n\tend\nend\n\nif Block.Mask.tmode[1] == 1 then\n\tpin_init(Block"
".Mask.tim[1], Block.Mask.pin0[1], Block.Mask.pin2[1], Block.Mask.pre0[1], Blo"
"ck.Mask.pre2[1])\nelse if Block.Mask.tmode[1] == 2 then\n\tinvert_init(Block."
"Mask.tim[1], Block.Mask.out0[1], Block.Mask.out2[1], Block.Mask.invert[1], Bl"
"ock.Mask.pre0[1], Block.Mask.pre2[1])\n\n\nreturn {\n  Include = \"analog_Con"
"fig.h\",\n  InitCode = InitCode,\n  OutputCode = OutputCode,\n  Require = Req"
"uire\n}\n\n"
          MaskType      "PWM Out"
